<h3>Тест <small><%= @question.online_test.title %></small></h3>
<div class="question">

  <%=form_tag(user_replies_path(queue: @queue), method: "post") do %>

    <ul class="list-group">
      <li class="list-group-item active"> <%= @question.title %> </li>
      <% @answers.each do |answer| %>
        <li class="list-group-item">
          <%= check_box_tag 'answer_ids[]', answer.id, @cart.user_replies.map(&:answer).include?(answer), :id => "answer_ids_#{answer.id}" -%>
          <%= answer.text %>

         </li>
      <% end %>
    </ul>
      <div class="form-group">
        <div class="col-sm-offset-2 col-sm-10">
          <%=submit_tag "Далее", class: "btn btn-info"%>
        </div>
       </div>
   <%end%>
</div>